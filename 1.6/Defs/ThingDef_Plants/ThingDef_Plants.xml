    <!-- Carrot (Farmable) -->

    <ThingDef ParentName="PlantBase">
        <defName>FCP_Plant_Carrot</defName>
        <label>Carrot</label>
        <description>A nutritious root vegetable cultivated by survivors for its sweet flavor and versatility. Carrots are a staple crop in many wasteland settlements.</description>
        <statBases>
            <MaxHitPoints>80</MaxHitPoints>
            <Nutrition>0.25</Nutrition>
        </statBases>
        <graphicData>
            <texPath>Things/Plants/Carrot</texPath>
            <graphicClass>Graphic_Random</graphicClass>
            <shadowData>
                <volume>(0.1, 0.3, 0.1)</volume>
            </shadowData>
        </graphicData>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <fillPercent>0.10</fillPercent>
        <pathCost>20</pathCost>
        <ingestible />
        <plant>
            <sowTags>
                <li>Ground</li>
                <li>Hydroponic</li>
            </sowTags>
            <immatureGraphicPath>Things/Plants/Carrot_Immature</immatureGraphicPath>
            <fertilitySensitivity>0.6</fertilitySensitivity>
            <growDays>4</growDays>
            <harvestTag>Standard</harvestTag>
            <harvestAfterGrowth>0.45</harvestAfterGrowth>
            <harvestedThingDef>FCP_Raw_Carrot</harvestedThingDef>
            <harvestYield>8</harvestYield>
            <harvestWork>60</harvestWork>
            <visualSizeRange>0.7~1.1</visualSizeRange>
            <topWindExposure>0.1</topWindExposure>
            <purpose>Food</purpose>
        </plant>
    </ThingDef>

