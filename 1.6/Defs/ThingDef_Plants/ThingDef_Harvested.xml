    <ThingDef ParentName="PlantFoodRawBase">
        <defName>FCP_Raw_Carrot</defName>
        <label>carrot</label>
        <description>A sweet, nutritious root vegetable harvested from carrot plants. A staple food in the wasteland.</description>
